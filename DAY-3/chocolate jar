l=list(map(int,input().split()))
n=int(input())
a=0
b=0
c=0
for i in l:
     a=a+(i//3)
     b=b+(i//3)
     c=c+(i//3)
     if i%3!=0:
           a=a+1
           b=b+1
           c=c+1
     else:
           a=a+0
           b=b+0
           c=c+0
print(a)
print(b)
print(c)
         
         
         
